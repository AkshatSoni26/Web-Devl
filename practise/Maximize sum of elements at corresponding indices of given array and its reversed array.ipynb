    "%%time\n",
    "\n",
    "arr = [1, 8, 9, 5, 4, 6 ]\n",
    "\n",
    "arr_len = len(arr)\n",
    "maxi = 0\n",
    "i =0\n",
    "\n",
    "n = 0\n",
    "\n",
    "if (arr_len%2 == 0):\n",
    "    n = arr_len%2 \n",
    "else:\n",
    "    n = (int(arr_len%2)+1)\n",
    "\n",
    "\n",
    "while (0 < n):\n",
    "    if (maxi < arr[i] + arr[arr_len - i - 1]):\n",
    "        maxi = arr[i] + arr[arr_len - i - 1]\n",
    "    i +=1\n",
    "\n",
    "\n",
    "\n",
    "print(maxi)"
   ]
